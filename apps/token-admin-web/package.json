{
  "name": "token-admin-web",
  "version": "1.0.0",
  "description": "wallet admin console",
  "private": true,
  "scripts": {
    "releaseas": "npx standard-version --release-as",
    "release:show": "npx standard-version --dry-run",
    "release:patch": "npx standard-version --release-as patch",
    "release:minor": "npx standard-version --release-as minor",
    "release:major": "npx standard-version --release-as major",
    "postJSMajorVersion": "npm version major",
    "postJSMinorVersion": "npm version minor",
    "postJSPatchVersion": "npm version patch",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject"
  },
  "dependencies": {
    "@date-io/date-fns": "1.x",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "flux-constants": "^0.2.2",
    "history": "^5.0.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.20",
    "perfect-scrollbar": "^1.4.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-datepicker": "^3.4.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reset-css": "^5.0.1",
    "styled-components": "^5.2.1",
    "yup": "0.32.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@testing-library/react": "^11.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-react-app": "^6.0.0",
    "husky": "^4.2.3",
    "jest-jenkins-reporter": "^1.0.2",
    "jest-junit": "^12.0.0",
    "jsdom": "^16.4.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga-test-plan": "^4.0.1",
    "sinon": "^9.2.4",
    "three": "^0.125.2",
    "worker-loader": "^3.0.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.json": [
      "prettier --parser json-stringify --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/serviceWorker.js",
      "!src/index.js",
      "!src/**/*.d.ts",
      "!src/**/__stories__/",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "text",
      "html"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
